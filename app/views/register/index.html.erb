<div class="top_register"></div>
<%= form_for 'register_form',:url => {:controller=>"register",:action => "register"} do |f| %>
<div class="register">
  <div class="logo_register">
    <%= image_tag("rails.png") %>
  </div>
  <table class="ipt_box">
    <tr>
      <td><%= f.label "用户名" %><span class="required"> *</span></td>
    <td><%= f.text_field :name,:required => true %></td>
    </tr>
    <tr><td>&nbsp;</td></tr>
    <tr>
    <td><%= f.label "密码" %><span class="required"> *</span></td>
    <td><%= f.password_field :password %></td>
    </tr>
    <tr><td>&nbsp;</td></tr>
    <tr>
    <td><%= f.label "密码确认" %><span class="required"> *</span></td>
    <td><%= f.password_field :password_again %></td>
    </tr>
    <tr><td>&nbsp;</td></tr>
    <tr>
    <td><%= f.label "邮箱" %><span class="required"> *</span></td>
    <td><%= f.text_field :email %></td>
    </tr>
    <tr><td>&nbsp;</td></tr>
    <tr>
    <td><%= f.label "单位" %></td>
    <td><%= f.text_field :company %></td>
    </tr>
  </table></div>
  <br/>
  <%= f.submit "login"  %>
<% end %>
